{
  "name": "dex-backend",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomochain/dex-server.git"
  },
  "scripts": {
    "start-env": "docker-compose up -d rabbitmq redis mongodb",
    "hot": "BIN_APP_PORT=8082 GIN_PORT=8080 GIN_BUILD_ARGS=server.go gin run serve",
    "seeds": "docker-compose up mongodb-seed",
    "stop-env": "docker-compose down",
    "genesis-token": "go run utils/seed-data/main.go genesis-token"
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=2.15.9"
  },
  "license": "Apache-2.0"
}
